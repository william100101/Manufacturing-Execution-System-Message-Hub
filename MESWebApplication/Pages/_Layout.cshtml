    <link rel="stylesheet" href="css/siteCustom.css" />
    <link rel="stylesheet" href="css/simple.css" />
    <link rel="stylesheet" href="lib/prettify/prettify.css" />
    <link rel="stylesheet" href="css/sunburst.css" />
    <link rel="stylesheet" href="MESWebApplication.styles.css" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" />
    <link href="MESWebApplication.styles.css" rel="stylesheet" />
    <link href="@Url.Content("lib/blazor-ui/styles/bootstrap-main.css")" rel="stylesheet" type="text/css" />
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
</head>
<body id="page-top" onload="PR.prettyPrint()">
    @RenderBody()
